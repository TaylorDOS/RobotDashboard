{
  "compilerOptions": {
    "baseUrl": ".", // Ensure this is set to the project root
    "paths": {
      "@/*": ["src/*"] // This assumes your 'src' folder exists at the project root
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "target": "ES2017"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "pages/api/sse3/route.js"  ],
  "exclude": [
    "node_modules"
  ]
}
